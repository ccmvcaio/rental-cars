<h1>Modelos de carro</h1>
<%= link_to 'Voltar', root_path %>

<% if @car_models.empty? %>
  <h3>Nenhum modelo de carro cadastrado</h3>
<% end %>

<% @car_models.each do |car_model|%>
  <ul>
  <li><%= link_to "#{car_model.car_category.name}", car_model%></li>
  </ul>
<% end %>

<%= link_to 'Registrar um modelo de carro', new_car_model_path %>